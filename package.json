    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "lint-staged": "11.0.0",
    "next-sitemap": "^3.1.52",
    "prettier": "2.3.0",
    "shx": "^0.3.3",
    "stripe": "^8.184.0",
    "stylelint": "^15.10.1",
    "stylelint-config-sass-guidelines": "9.0.1",
    "typescript": "5.1.6"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "yarn run lint"
    ],
    "*.{ts,tsx,js,json}": [
      "yarn run format"
    ],
    "*.scss": "yarn run lint:styles"
  },
  "browser": {
    "html-pdf": false
  },
  "resolutions": {
    "postcss": "^8.4.31",
    "zod": "^3.22.3"
  }
